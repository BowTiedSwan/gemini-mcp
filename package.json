{
  "name": "gemini-mcp",
  "version": "1.0.0",
  "description": "MCP server for Google Gemini App with session support and human-like behavior",
  "type": "module",
  "bin": {
    "gemini-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/index.js",
    "watch": "tsc --watch",
    "dev": "tsx watch src/index.ts",
    "prepare": "npm run build",
    "test": "tsx src/index.ts"
  },
  "keywords": [
    "mcp",
    "gemini",
    "google",
    "ai",
    "claude"
  ],
  "author": "Adapted from gemini-mcp by Gérôme Dexheimer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BowTiedSwan/gemini-mcp.git"
  },
  "homepage": "https://github.com/BowTiedSwan/gemini-mcp#readme",
  "bugs": {
    "url": "https://github.com/BowTiedSwan/gemini-mcp/issues"
  },
  "files": [
    "dist",
    "README.md",
    "CONVERSATIONLM_USAGE.md",
    "LICENSE",
    "docs"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^16.4.0",
    "env-paths": "^3.0.0",
    "globby": "^14.1.0",
    "patchright": "^1.48.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
